{
        "modulePrefix": "lemonade-ember",
        "environment": "production",
        "rootURL": "/",
        "locationType": "auto",
        "EmberENV": {
                "FEATURES": {},
                "EXTEND_PROTOTYPES": {
                        "Date": false
                }
        },
        "APP": {
                "name": "lemonade-ember",
                "version": "0.0.0+"
        },
        "ember-simple-auth": {
                "authorizer": "authorizer:devise",
                "routeAfterAuthentication": "/home/workflows"
        },
        "thorn": "/thorn",
        "stand": "/stand",
        "tahiti": "/tahiti",
        "webSocketIO": {
                "url": "/stand",
                "namespace": "/stand",
                "path": "/stand/socket.io"
        },
        "exportApplicationGlobal": false
}

